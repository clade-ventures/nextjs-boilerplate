{
  "name": "@clade-ventures/nextjs-boilerplate",
  "description": "A well-structured production ready Next.js boilerplate with Typescript, Redux, Jest, React Testing Library, Cypress, EnvConfig, Fetch, Bundle Analyzer. https://github.com/clade-ventures/nextjs-boilerplate",
  "version": "1.0.0",
  "repository": "https://github.com/clade-ventures/nextjs-boilerplate",
  "author": "HZN Engineer",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start:dev": "next dev",
    "start:doc": "start-storybook -s ./public/static -p 6006",
    "start:analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "test:unit": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "export": "cross-env NODE_ENV=production next export",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@next/bundle-analyzer": "^11.1.2",
    "@reduxjs/toolkit": "^1.6.1",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^12.1.0",
    "next-i18next": "^8.7.0",
    "next-redux-wrapper": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.1",
    "react-redux": "^7.2.5",
    "redux-persist": "^6.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.3.8",
    "@storybook/addon-viewport": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@storybook/source-loader": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.20",
    "@types/react-redux": "^7.1.18",
    "@types/redux-mock-store": "^1.0.3",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^3.5.1",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-ts": "^0.4.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-flowtype": "^5.9.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "jest": "^27.1.0",
    "nock": "^13.1.3",
    "prettier": "^2.3.2",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.1.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "license": "ISC"
}